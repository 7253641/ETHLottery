#!/bin/bash

. bin/compile

sed -e 's/^var lottery_code.*/var lotttery_code = "0x'$CODE'";/' \
    -e 's/^var lottery_abi.*/var lottery_abi = '$ABI';/' -i bin/admin.js

. bin/compile_manager

sed -e 's/^var manager_code.*/var manager_code = "0x'$CODE'";/' \
    -e 's/^var manager_abi.*/var manager_abi = '$ABI';/' -i bin/admin.js
