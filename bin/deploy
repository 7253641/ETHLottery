#!/bin/bash


geth=~/go-ethereum/build/bin/geth
date=$(date +%s)
deploy_file=bin/deploy_${date}.js

# TODO make args of those vars
fee=1
jackpot=10
owner_fee=2
owner="0xe581c6f0fae3bc426acfc660b36a7caf90f27987"

. bin/compile

sed -e 's/OWNER_FEE/'$owner_fee'/' \
    -e 's/FEE/'$fee'/' \
    -e 's/JACKPOT/'$jackpot'/' \
    -e 's/ABI/'$abi'/' \
    -e 's/CODE/'$code'/' \
    -e 's/OWNER/'$owner'/' bin/deploy_contract.tpl.js > $deploy_file

$geth --testnet --datadir ~/eth-testnet js $deploy_file --unlock $owner
