#!/bin/bash

geth=/usr/src/go-ethereum/build/bin/geth
date=$(date +%s)
deploy_file=managers/deploy_manager_${date}.js

owner="0xe581c6f0fae3bc426acfc660b36a7caf90f27987"

. bin/compile_manager

sed -e 's/ABI/'$abi'/' \
    -e 's/CODE/'$code'/' \
    -e 's/OWNER/'$owner'/' bin/deploy_manager.tpl.js > $deploy_file

$geth --testnet --datadir ~/eth-testnet js $deploy_file --unlock $owner
